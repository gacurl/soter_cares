<% if current_page?(root_path) || current_page?(start_journies_path) || current_page?(living_journies_path) %>
  <head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Soter</title>
    
    <!-- Bootstrap core CSS -->
    <link href="https://cdn.sotercares.com/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdn.sotercares.com/css/user.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.sotercares.com/css/font-awesome.min.css">
    <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
