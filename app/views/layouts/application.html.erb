     <% flash.each do |message_type, message| %>
        <%= content_tag(:div, class: "alert alert-#{message_type}") do %>
          <%= message %>
        <% end %>
      <% end %>
     <%= yield %>
    </div>
        <footer class="py-5 bg-dark">
        <div class="container">
            <p class="m-0 text-center text-white" style="color:white">Copyright &copy; <%= Time.now.strftime('%Y') %> - Soter, LLC</p>
        </div>
        <!-- /.container -->
    </footer>
      
      
    </div>
<div id="modal-window" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>


