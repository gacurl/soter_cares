    <!-- Custom styles for this template -->
    <link href="https://cdn.sotercares.com/css/modern-business.css" rel="stylesheet">

</head>

<body>

    <!-- Navigation -->
    <nav class="navbar fixed-top navbar-expand-lg navbar-light bg-light fixed-top">
        <%= link_to "#{image_tag("https://cdn.sotercares.com/images/Soter-Logo.png", border: 0, height: 30)} SOTER".html_safe, root_url, {class: 'navbar-brand'} %>
        <button class="navbar-toggler navbar-toggler-right navbar-toggler-inverse" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <%= link_to "Employee Login", login_path, class: "nav-link" if current_page?(root_path) %>
                </li>
            </ul>
        </div>
    </nav>
    <div style="background-color: #f5f5f5">
     <%= yield %>
    </div>
    <div id="modal-window" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>
    <!-- Footer -->
    <footer class="py-5 bg-dark">
        <div class="container">
            <h3 class="m-0 text-center text-white">Call Us: <%= link_to "321.419.0041", "tel:3214190041" %></h3>
            <h3 class="m-0 text-center text-white">Email Us: <%= link_to "info@sotercares.com", "mailto:info@sotercares.com" %></h3>
            <p class="m-0 text-center text-white">2197 Julian Ave NE</p>
            <p class="m-0 text-center text-white">Palm Bay, FL 32905</p>
            <p class="m-0 text-center text-white">Copyright &copy; <%= Time.now.strftime('%Y') %> - Soter, LLC</p>
        </div>
        <!-- /.container -->
    </footer>
    <!-- Bootstrap core JavaScript -->
    <script src="https://cdn.sotercares.com/vendor/popper/popper.min.js"></script>
    <script src="https://cdn.sotercares.com/vendor/bootstrap/js/bootstrap.min.js"></script>
    
</body>

<% elsif logged_in? %>
<head>
    <title><%= full_title(yield(:title)) %></title>
    <%= stylesheet_link_tag 'application', media: 'all',
      'data-turbolinks-track' => true %>
    <link rel="stylesheet" href="https://cdn.sotercares.com/css/font-awesome.min.css">
    <%= javascript_include_tag "https://www.google.com/jsapi", "chartkick" %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <script src="//maps.google.com/maps/api/js?key=<%= Rails.application.secrets.gmap %>"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="container-fluid">
        <% if logged_in? %>
          <%= render partial: 'layouts/sidebar' %>
        <% end %>
        <div class="col-md-12">
          <div id="page-wrapper">
          
            <% flash.each do |message_type, message| %>
              <%= content_tag(:div, class: "alert alert-#{message_type}") do %>
                <%= message %>
              <% end %>
            <% end %>
            
            <%= yield %>
          </div>
        </div>
        <div class="col-md-12">
          <div id="page-wrapper">
            
              <%= render 'layouts/footer' %>
            <%= debug(params) if Rails.env.development? %>
          </div>
        </div>
      
      
    </div>
<div id="modal-window" class="modal fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>

<script>    
  $(document).on('turbolinks:load', function() {
     $(window).trigger('load.bs.select.data-api');
     $(".image-button").filestyle({
      badge: false,
      input: true,
      buttonBefore: true,
      buttonName: "btn-default",
      buttonText: "Add Image",
      iconName: "glyphicon glyphicon-camera"
    });
    $(".file-button").filestyle({
      badge: false,
      input: true,
      buttonBefore: true,
      buttonName: "btn-default",
      buttonText: "Add File",
      iconName: "glyphicon glyphicon-folder-open"
    });
  });
</script>


<% else  %>
