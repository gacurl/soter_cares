
    
      <% object.data_files.each do |data_file| %>
            <% disposition = "attachment; filename=#{data_file.file.original_filename.inspect}" %>
          <tr>
            <td><%= link_to data_file.file.data["metadata"]["filename"], 
                        data_file.file.url(response_content_disposition: disposition, 
                        virtual_host: true, host: "https://storage.sotercares.com"), 
                        download: true %></td>
            <td><%= data_file.created_at.strftime("%m/%d/%Y-%T") %></td>
            <td><%= data_file.user.name %></td>
          </tr>
      <% end %>
